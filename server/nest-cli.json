{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/server/src",
  "monorepo": true,
  "root": "apps/server",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/server/tsconfig.app.json"
  },
  "projects": {
    "server": {
      "type": "application",
      "root": "apps/server",
      "entryFile": "main",
      "sourceRoot": "apps/server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/server/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "service": {
      "type": "application",
      "root": "apps/service",
      "entryFile": "main",
      "sourceRoot": "apps/service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/service/tsconfig.app.json"
      }
    },
    "client": {
      "type": "application",
      "root": "apps/client",
      "entryFile": "main",
      "sourceRoot": "apps/client/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client/tsconfig.app.json"
      }
    },
    "neo4j": {
      "type": "library",
      "root": "libs/neo4j",
      "entryFile": "index",
      "sourceRoot": "libs/neo4j/src",
      "compilerOptions": {
        "tsConfigPath": "libs/neo4j/tsconfig.lib.json"
      }
    },
    "tencentcloud": {
      "type": "library",
      "root": "libs/tencentcloud",
      "entryFile": "index",
      "sourceRoot": "libs/tencentcloud/src",
      "compilerOptions": {
        "tsConfigPath": "libs/tencentcloud/tsconfig.lib.json"
      }
    },
    "sbd": {
      "type": "library",
      "root": "libs/sbd",
      "entryFile": "index",
      "sourceRoot": "libs/sbd/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sbd/tsconfig.lib.json"
      }
    },
    "xlsx": {
      "type": "library",
      "root": "libs/xlsx",
      "entryFile": "index",
      "sourceRoot": "libs/xlsx/src",
      "compilerOptions": {
        "tsConfigPath": "libs/xlsx/tsconfig.lib.json"
      }
    }
  }
}